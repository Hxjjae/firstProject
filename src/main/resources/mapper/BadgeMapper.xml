<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.green.firstproject.badge.BadgeMapper">


    <insert id="insBadge">
        Insert into Badge
        (points)
        values
        (#{points})
    </insert>


    <update id="insPic">
        Update
        badge
        set
        pic = #{pic}
        where ibadge = #{ibadge}
    </update>
</mapper>